{
  "name": "EDA-Quest",
  "version": "1.0.0",
  "description": "A rouge-like dungeon crawler game that is themed around learning to code",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "dev:start": "nodemon server/server.js",
    "postinstall": "npm run build",
    "build": "browserify . -o public/bundle.js -g uglifyify",
    "watch": "watchify . -o public/bundle.js -d",
    "test": "npm run test:reducers && npm run test:integration",
    "test:reducers": "browserify ./tests/*.js | tape | tap-spec",
    "test:integration": "chimp --watch"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "engines": {
    "node": "6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quest-friends/dev-quest"
  },
  "author": "Hudson Prestidge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quest-friends/dev-quest/issues"
  },
  "homepage": "https://github.com/quest-friends/dev-quest",
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.7",
    "browserify": "^13.0.0",
    "express": "^4.14.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "chimp": "^0.40.6",
    "cucumber-assert": "^1.0.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  }
}
